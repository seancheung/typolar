{
    "name": "typolar",
    "version": "0.3.3",
    "description": "A simple typescript framework",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "prebuild": "del-cli \"lib/**\" \"!lib\" \"!.gitignore\"",
        "build": "cross-env NODE_ENV=production tsc -p tsconfig.prod.json",
        "lint": "tslint 'src/**/*.ts' 'test/**/*.ts'",
        "pretest": "npm run lint",
        "test": "cross-env NODE_ENV=test LOG_LEVEL=off TS_NODE_FILES=true mocha -r ts-node/register 'test/**/*.spec.ts'"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/seancheung/typolar.git"
    },
    "keywords": [
        "framework",
        "typescript"
    ],
    "author": "Sean Cheung",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/seancheung/typolar/issues"
    },
    "homepage": "https://github.com/seancheung/typolar#readme",
    "devDependencies": {
        "cross-env": "^5.2.0",
        "@types/morgan": "^1.7.35",
        "@types/express": "^4.16.0",
        "@types/request-promise": "^4.1.42",
        "del-cli": "^1.1.0",
        "mocha": "^5.2.0",
        "prettier": "^1.14.3",
        "ts-node": "^7.0.1",
        "tslint": "^5.11.0",
        "tslint-config-prettier": "^1.15.0",
        "tslint-consistent-codestyle": "^1.13.3",
        "typescript": "^3.1.3"
    },
    "dependencies": {
        "body-parser": "^1.18.3",
        "express": "^4.16.4",
        "kuconfig": "^0.1.3",
        "log4js": "^3.0.6",
        "mockit-express": "^0.4.0",
        "morgan": "^1.9.1",
        "request": "^2.88.0",
        "request-promise": "^4.2.2",
        "stringcase": "^4.3.0"
    }
}
